                     <span class="navbar-toggler-icon"></span>
                  </button>
                  <div class="collapse navbar-collapse" id="navbarsExample04">
                            @if (User.Identity.IsAuthenticated)
                            {
                                @if (User.IsInRole("Admin"))
                                {
                                    <ul class="navbar-nav mr-auto">
                                        <li class="nav-item">
                                            <a class="nav-link " asp-area="" asp-controller="Users" asp-action="Index">Users</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" onclick="location.href='@Url.Action("Create", "RespserviceAchats")'">Add RespServiceAchat</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" onclick="location.href='@Url.Action("Create", "RespServicequalites")'">Add RespServicequalite</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" onclick="location.href='@Url.Action("Create", "RespServiceFinances")'">Add RespServiceFinances</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" onclick="location.href='@Url.Action("Create", "ReceptServiceAchats")'">Add RecepServicAchats</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" onclick="location.href='@Url.Action("Logout", "Users")'">Logout</a>
                                        </li>
                                    </ul>
                                }
                                else
                                {
                                    <ul class="navbar-list">
                                        <li class="navbar-item">
                                            <a class="navbar-link skewBg" asp-area="" asp-controller="Produits" asp-action="List">Games</a>
                                        </li>
                                        <li class="navbar-item">
                                            <button class="navbar-button skewBg" onclick="location.href='@Url.Action("Logout", "Users")'">Logout</button>
                                        </li>
                                    </ul>
                                }
                            }


                     </ul>
                  </div>
               </nav>
            </div>

         </div>
      </div>
   </div>
